<?xml version="1.0"?>
<!DOCTYPE ejb-jar PUBLIC '-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN' 'http://java.sun.com/dtd/ejb-jar_2_0.dtd'>
<ejb-jar id="ejb-jar_ID">
  <display-name>module one</display-name>
  <enterprise-beans>
    <session id="Werner">
      <ejb-name>Werner</ejb-name>
      <home>testPackage.TestHome</home>
      <remote>testPackage.TestRemote</remote>
      <local-home>testPackage.TestLocalHome</local-home>
      <local>testPackage.TestLocal</local>
      <ejb-class>testPackage.TestClass</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Bean</transaction-type>
      <env-entry>
        <env-entry-name>BooleanEnvVar</env-entry-name>
        <env-entry-type>java.lang.Boolean</env-entry-type>
        <env-entry-value>true</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>StringEnvVar</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>abcdefghijklmnopqrstuvwxyz</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>CharacterEnvVar</env-entry-name>
        <env-entry-type>java.lang.Character</env-entry-type>
        <env-entry-value>A</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>ByteEnvVar</env-entry-name>
        <env-entry-type>java.lang.Byte</env-entry-type>
        <env-entry-value>1</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>IntegerEnvVar</env-entry-name>
        <env-entry-type>java.lang.Integer</env-entry-type>
        <env-entry-value>1234567890</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>ShortEnvVar</env-entry-name>
        <env-entry-type>java.lang.Short</env-entry-type>
        <env-entry-value>8311</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>LongEnvVar</env-entry-name>
        <env-entry-type>java.lang.Long</env-entry-type>
        <env-entry-value>1234567890123456789</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>FloatEnvVar</env-entry-name>
        <env-entry-type>java.lang.Float</env-entry-type>
        <env-entry-value>12.34</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>DoubleEnvVar</env-entry-name>
        <env-entry-type>java.lang.Double</env-entry-type>
        <env-entry-value>1234567890.123456789</env-entry-value>
      </env-entry>
      <!-- remote ejb reference to bean in the same module -->
      <ejb-ref>
        <ejb-ref-name>ejb/Roger</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <home>testPackage.TestHome</home>
        <remote>testPackage.TestRemote</remote>
        <ejb-link>Roger</ejb-link>
      </ejb-ref>
      <!-- local ejb reference to bean in the same module -->
      <ejb-local-ref>
        <ejb-ref-name>ejb/Harald</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local-home>testPackage.TestLocalHome</local-home>
        <local>testPackage.TestLocal</local>
        <ejb-link>Harald</ejb-link>
      </ejb-local-ref>
    </session>
    <session id="Harald">
      <ejb-name>Harald</ejb-name>
      <home>testPackage.TestHome</home>
      <remote>testPackage.TestRemote</remote>
      <local-home>testPackage.TestLocalHome</local-home>
      <local>testPackage.TestLocal</local>
      <ejb-class>testPackage.TestClass</ejb-class>
      <session-type>Stateful</session-type>
      <transaction-type>Container</transaction-type>
      <!-- remote ejb reference to bean in another module in the same J2EE application (with module prefix) -->
      <ejb-ref>
        <ejb-ref-name>ejb/JuergWithModulePrefix</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <home>testPackage.TestHome</home>
        <remote>testPackage.TestRemote</remote>
        <ejb-link>two.jar#Juerg</ejb-link>
      </ejb-ref>
      <!-- remote ejb reference to bean in another module in the same J2EE application (without module prefix) -->
      <ejb-ref>
        <ejb-ref-name>ejb/JuergWithoutModulePrefix</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <home>testPackage.TestHome</home>
        <remote>testPackage.TestRemote</remote>
        <ejb-link>Juerg</ejb-link>
      </ejb-ref>
      <!-- local ejb reference to bean in another module in the same J2EE application (with module prefix) -->
      <ejb-local-ref>
        <ejb-ref-name>ejb/MartinWithModulePrefix</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local-home>testPackage.TestLocalHome</local-home>
        <local>testPackage.TestLocal</local>
        <ejb-link>two.jar#Martin</ejb-link>
      </ejb-local-ref>
      <!-- local ejb reference to bean in another module in the same J2EE application (without module prefix) -->
      <ejb-local-ref>
        <ejb-ref-name>ejb/MartinWithoutModulePrefix</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local-home>testPackage.TestLocalHome</local-home>
        <local>testPackage.TestLocal</local>
        <ejb-link>Martin</ejb-link>
      </ejb-local-ref>
    </session>
    <session id="Roger">
      <ejb-name>Roger</ejb-name>
      <home>testPackage.TestHome</home>
      <remote>testPackage.TestRemote</remote>
      <local-home>testPackage.TestLocalHome</local-home>
      <local>testPackage.TestLocal</local>
      <ejb-class>testPackage.TestClass</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
      <!-- remote ejb reference to bean in another module in another J2EE application -->
      <ejb-ref>
        <ejb-ref-name>ejb/Daniel</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <home>testPackage.TestHome</home>
        <remote>testPackage.TestRemote</remote>
      </ejb-ref>
      <!-- remote ejb reference to bean in the same module (ejb with same name also exists in another module) -->
      <ejb-ref>
        <ejb-ref-name>ejb/Harald</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <home>testPackage.TestHome</home>
        <remote>testPackage.TestRemote</remote>
        <ejb-link>Harald</ejb-link>
      </ejb-ref>
      <!-- local ejb reference to bean in another module in another J2EE application -->
      <ejb-local-ref>
        <ejb-ref-name>ejb/Markus</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local-home>testPackage.TestLocalHome</local-home>
        <local>testPackage.TestLocal</local>
      </ejb-local-ref>
      <!-- local ejb reference to bean in the same module (ejb with same name also exists in another module) -->
      <ejb-local-ref>
        <ejb-ref-name>ejb/Werner</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local-home>testPackage.TestLocalHome</local-home>
        <local>testPackage.TestLocal</local>
        <ejb-link>Werner</ejb-link>
      </ejb-local-ref>
      <!-- resource reference -->
      <resource-ref>
        <res-ref-name>resource/TestResourceRef</res-ref-name>
        <res-type>TestResourceRefType</res-type>
        <res-auth>Container</res-auth>
        <res-sharing-scope>Shareable</res-sharing-scope>
      </resource-ref>
      <!-- resource environment reference -->
      <resource-env-ref>
        <resource-env-ref-name>resource-env/TestResourceEnvRef</resource-env-ref-name>
        <resource-env-ref-type>TestResourceEnvRefType</resource-env-ref-type>
      </resource-env-ref>
    </session>
  </enterprise-beans>
  <assembly-descriptor>
    <container-transaction>
	  <method>
        <ejb-name>Roger</ejb-name>
	    <method-intf>Remote</method-intf>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>
  </assembly-descriptor>
</ejb-jar>
