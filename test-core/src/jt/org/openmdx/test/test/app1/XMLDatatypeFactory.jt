package org.openmdx.test.test.app1;

import java.util.GregorianCalendar;

import javax.xml.datatype.DatatypeFactory;
import javax.xml.datatype.XMLGregorianCalendar;

/**
 * The XML Datatype Factory delegates to the platform specific 
 * Datatype Factory.
 */
public class XMLDatatypeFactory {
  
    public XMLDatatypeFactory (
    ) throws Exception {
        this.delegate = DatatypeFactory.newInstance();
    }
    
    private final DatatypeFactory delegate;

    public XMLGregorianCalendar newXMLGregorianCalendar(
        String a0
    ){
        return this.delegate.newXMLGregorianCalendar(a0);
    }

    public XMLGregorianCalendar newXMLGregorianCalendar(
        GregorianCalendar a0
    ){
        return this.delegate.newXMLGregorianCalendar(a0);
    }

    public XMLGregorianCalendar newXMLGregorianCalendarDate(
        int a0, int a1, int a2, int a3
    ){
        return this.delegate.newXMLGregorianCalendarDate(a0,a1,a2,a3);
    }

}
