package org.openmdx.test.test.app1.plugin.jmi;

import org.openmdx.base.accessor.jmi.cci.RefObject_1_0;
import org.openmdx.base.accessor.jmi.spi.RefObject_1;

#if defined(BEFORE16)
import java.util.Set;
import org.openmdx.test.app1.cci.Invoice;
#else
import org.openmdx.test.app1.jmi1.Invoice;
#endif

/**
 * Invoice Implementation
 */
public class InvoiceImpl
	extends RefObject_1
{
  
    /**
	 * Implements <code>Serializable</code>
	 */
	private static final long serialVersionUID = -1963425848064717131L;

    /**
     * Constructor 
     *
     * @param delegate
     */
    public InvoiceImpl(
        Invoice delegate
    ) {
        super(
            ((RefObject_1_0)delegate).refDelegate(), // TODO
            delegate.refClass()
        );
        this.delegate = delegate;
    }

    /**
     * 
     */
    private final Invoice delegate;

#if defined(BEFORE16)    
	public Set refDefaultFetchGroup(
	) {
        Set group = super.refDefaultFetchGroup();
        group.add("invoicePosition");
        return group;
 	}
#else
    // TODO
    // add "invoicePosition" to default fetch group
#endif

}

