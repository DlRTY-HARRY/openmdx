<ejb-jar 
	xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd"
	version="2.1"
	id="ejb-jar_ID"
>
  <enterprise-beans>
    <!-- deployment settings for gateway_NoOrNew -->
    <session>
      <ejb-name>gateway1-NoOrNew</ejb-name>
		<home>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1Home</home>
		<remote>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1_0Remote</remote>
		<local-home>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1LocalHome</local-home>
		<local>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1_0Local</local>
        <ejb-class>org.openmdx.compatibility.application.dataprovider.transport.ejb.transaction.Hub_1Bean</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Bean</transaction-type>
      <!-- local ejb reference to gateway gateway_Mandatory -->
      <ejb-local-ref>
        <ejb-ref-name>ejb/Mandatory</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local-home>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1LocalHome</local-home>
        <local>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1_0Local</local>
        <ejb-link>gateway1-Mandatory</ejb-link>
      </ejb-local-ref>
      <!-- local ejb reference to gateway gateway_Never -->
      <ejb-local-ref>
        <ejb-ref-name>ejb/Never</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local-home>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1LocalHome</local-home>
        <local>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1_0Local</local>
        <ejb-link>gateway1-Never</ejb-link>
      </ejb-local-ref>
    </session>
    <!-- deployment settings for gateway_Mandatory-->
    <session>
      <ejb-name>gateway1-Mandatory</ejb-name>
			<home>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1Home</home>
			<remote>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1_0Remote</remote>
			<local-home>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1LocalHome</local-home>
			<local>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1_0Local</local>
			<ejb-class>org.openmdx.compatibility.application.dataprovider.transport.ejb.server.Dataprovider_1Bean</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
      <env-entry>
        <env-entry-name>KERNEL/exposedPath[0]</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>xri:@openmdx:org.openmdx.test.app1/provider/ch:omex:test:junit/segment</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>KERNEL/namespaceId</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>explorer</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>KERNEL/INTERCEPTION</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>org.openmdx.compatibility.base.dataprovider.spi.DelegatingLayer_0</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>KERNEL/TYPE</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>org.openmdx.compatibility.base.dataprovider.spi.DelegatingLayer_0</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>KERNEL/APPLICATION</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>org.openmdx.compatibility.runtime1.layer.application.Explorer_1</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>KERNEL/MODEL</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>org.openmdx.compatibility.base.dataprovider.spi.DelegatingLayer_0</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>KERNEL/PERSISTENCE</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>org.openmdx.compatibility.base.dataprovider.layer.persistence.delegation.Delegation_1</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>APPLICATION/runtimeSegment</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>xri:@openmdx:org.openmdx.compatibility.runtime1/provider/org:openmdx/segment/app</env-entry-value>
      </env-entry>
      <ejb-local-ref>
        <ejb-ref-name>ejb/dataprovider[0]</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local-home>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1LocalHome</local-home>
        <local>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1_0Local</local>
        <ejb-link>transaction-Persistent</ejb-link>
      </ejb-local-ref>
    </session>
    <!-- deployment settings for gateway-Never-->
    <session>
      <ejb-name>gateway1-Never</ejb-name>
			<home>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1Home</home>
			<remote>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1_0Remote</remote>
			<local-home>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1LocalHome</local-home>
			<local>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1_0Local</local>
			<ejb-class>org.openmdx.compatibility.application.dataprovider.transport.ejb.server.Dataprovider_1Bean</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
      <env-entry>
        <env-entry-name>KERNEL/exposedPath[0]</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>xri:@openmdx:org.openmdx.test.app1/provider/ch:omex:test:junit/segment</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>KERNEL/namespaceId</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>explorer</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>KERNEL/INTERCEPTION</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>org.openmdx.compatibility.base.dataprovider.spi.DelegatingLayer_0</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>KERNEL/TYPE</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>org.openmdx.compatibility.base.dataprovider.spi.DelegatingLayer_0</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>KERNEL/APPLICATION</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>org.openmdx.compatibility.runtime1.layer.application.Explorer_1</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>KERNEL/MODEL</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>org.openmdx.compatibility.base.dataprovider.spi.DelegatingLayer_0</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>KERNEL/PERSISTENCE</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>org.openmdx.compatibility.base.dataprovider.layer.persistence.delegation.Delegation_1</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>APPLICATION/runtimeSegment</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>xri:@openmdx:org.openmdx.compatibility.runtime1/provider/org:openmdx/segment/app</env-entry-value>
      </env-entry>
      <ejb-local-ref>
        <ejb-ref-name>ejb/dataprovider[0]</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local-home>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1LocalHome</local-home>
        <local>org.openmdx.compatibility.application.dataprovider.transport.ejb.cci.Dataprovider_1_0Local</local>
        <ejb-link>transaction-Persistent</ejb-link>
      </ejb-local-ref>
    </session>
  </enterprise-beans>
  <assembly-descriptor>
    <container-transaction>
      <method>
        <ejb-name>gateway1-Mandatory</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Mandatory</trans-attribute>
    </container-transaction>
    <container-transaction>
      <method>
        <ejb-name>gateway1-Never</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Never</trans-attribute>
    </container-transaction>
  </assembly-descriptor>
</ejb-jar>
