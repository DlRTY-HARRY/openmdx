			<local>org.openmdx.application.dataprovider.transport.ejb.cci.EntityManagerFactory_2_0Local</local>
			<ejb-class>org.openmdx.application.dataprovider.transport.ejb.server.EntityManagerFactory_2Bean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
			<env-entry>
