			</env-entry>
			<env-entry>
			  <env-entry-name>KERNEL/PERSISTENCE</env-entry-name>
			  <env-entry-type>java.lang.String</env-entry-type>
			  <env-entry-value>org.openmdx.compatibility.base.dataprovider.layer.persistence.delegation.Delegation_1</env-entry-value>
			</env-entry>
			<env-entry>
				<env-entry-name>TYPE/stateCapable[0]</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>xri://@openmdx*org.openmdx.compatibility.state1/provider/($..)/segment/($..)/state1Core/($..)</env-entry-value>
